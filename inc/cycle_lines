
# cycleway overlay

(highway=path |
    highway=footway |
    highway=pedestrian) & 
        (bicycle=dismount |
         bicycle=no)                     [0x10e07 resolution 24-24 continue]

highway=*
          & (cycleway=both |
             cycleway=lane |
             cycleway=track )            [0x10e08 resolution 24-24 continue]

highway=*
          & (cycleway:right=yes |
             cycleway:right=lane)        [0x10e09 resolution 24-24 continue]

highway=*
          & (cycleway:left=yes |
             cycleway:left=lane |
             cycleway=opposite |
             cycleway=opposite_lane |
             oneway:bicycle=no)           [0x10e0a resolution 24-24 continue]

